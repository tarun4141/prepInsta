
//solution-1
#include <iostream>
#include<bits/stdc++.h>
using namespace std;



int main(){
  int n1=36,n2=60,hcf=1;
  
  for(int i=1;i<=n1&&i<=n2;i++){
      if(n1%i==0 && n2%i==0){
          hcf=i;
      }
  }
  cout<<hcf;
  
    return 0;
}
//solution-2
#include <iostream>
#include<bits/stdc++.h>
using namespace std;



int main(){
  int n1=36,n2=60;
  int a=n1,b=n2;
  
 while(n1!=n2){
     if(n1>n2) n1-=n2;
     else n2-=n1;
 }
 
  cout<<n1;
  
    return 0;
}
