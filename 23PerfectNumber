#include <iostream>
#include<bits/stdc++.h>
using namespace std;



int main() {
    int n=28;
    int o=n;
    int sum=0;
    
    for(int i=1;i<n;i++){
        if(n%i==0){
            sum+=i;
        }
    }
    
    if(o==sum ) cout<< "yes"<<o<<" "<<sum;
    else cout<<"nahi";
    
    
   return 0;
}
