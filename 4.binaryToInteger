//solution-1
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int getD(int n){
    int i=0,d=0;
    while(n!=0){
        int digit=n%10;
        d+=digit*pow(2,i);
        n/=10;
        i++;
    }
    return d;
}



int main(){
  int b=11011;
  cout<<getD(b);
  
    return 0;
}

